[color]
	ui = true
[user]
	name = CloudNiner
	email = andy@finkmiller.com
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore
[delta]
	navigate = true
	syntax-theme = Dracula
[diff]
	colorMoved = default
[init]
	defaultBranch = main
[interactive]
	diffFilter = delta --color-only
[merge]
	conflictstyle = diff3
[pull]
	rebase = false
[push]
	default = simple
	autoSetupRemote = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
