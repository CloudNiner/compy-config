	ui = true
[user]
	name = CloudNiner
	email = andy@finkmiller.com
[core]
	editor = /usr/bin/vim
	excludesfile = ~/.gitignore
[push]
	default = simple
        autoSetupRemote = true
[pull]
	rebase = false
[merge]
	conflictstyle = diff3
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
